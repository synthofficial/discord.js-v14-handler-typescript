#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/synth/Documents/GitHub/Casette Full Rewrite/node_modules/.pnpm/supports-color@1.3.1/node_modules/supports-color/node_modules:/mnt/c/Users/synth/Documents/GitHub/Casette Full Rewrite/node_modules/.pnpm/supports-color@1.3.1/node_modules:/mnt/c/Users/synth/Documents/GitHub/Casette Full Rewrite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/synth/Documents/GitHub/Casette Full Rewrite/node_modules/.pnpm/supports-color@1.3.1/node_modules/supports-color/node_modules:/mnt/c/Users/synth/Documents/GitHub/Casette Full Rewrite/node_modules/.pnpm/supports-color@1.3.1/node_modules:/mnt/c/Users/synth/Documents/GitHub/Casette Full Rewrite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../supports-color@1.3.1/node_modules/supports-color/cli.js" "$@"
else
  exec node  "$basedir/../../../../../supports-color@1.3.1/node_modules/supports-color/cli.js" "$@"
fi
